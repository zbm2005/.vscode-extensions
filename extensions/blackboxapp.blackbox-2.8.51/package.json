				"title": "Open Multi File Editor Chat"
			}
		],
		"keybindings": [
			{
				"command": "extension.activateContext",
				"key": "ctrl+i",
				"mac": "cmd+i"
			},
			{
				"command": "extension.saveText",
				"key": "ctrl+alt+l",
				"mac": "shift+cmd+i"
			},
			{
				"command": "extension.acceptSuggestion",
				"key": "tab",
				"mac": "tab",
				"when": "inlineSuggestionHasIndentationLessThanTabSize && inlineSuggestionVisible && !editorTabMovesFocus"
			},
			{
				"command": "extension.saveCodeToCloud",
				"key": "ctrl+l",
				"mac": "cmd+L",
				"when": "editorTextFocus"
			}
		],
		"menus": {
			"view/item/context": [
				{
					"command": "extension.viewVersionDiff",
					"when": "viewItem == version",
					"group": "inline@1"
				},
				{
					"command": "extension.restoreVersion",
					"when": "viewItem == version",
					"group": "inline@2"
				}
			],
			"scm/title": [
				{
					"command": "blackbox.commitMsg",
					"when": "scmProvider == git",
					"group": "navigation",
					"title": "Commit Prefill"
				}
			],
			"commandPalette": [
				{
					"command": "blackbox.comment.add",
					"when": "false"
				},
				{
					"command": "blackbox.terminal.search",
					"when": "blackbox.newInstallation"
				}
			],
			"comments/commentThread/context": [
				{
					"command": "blackbox.comment.add",
					"group": "inline"
				}
			]
		}
	},
	"scripts": {
		"lint": "eslint . --ext .ts,.tsx",
		"watch": "tsc -watch -p ./",
		"download-api": "vscode-dts dev",
